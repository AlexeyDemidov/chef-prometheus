#!/bin/sh

exec 2>&1

export GOMAXPROCS=<%= node['cpu']['total'] %>

<%
config = ''
node['prometheus']['flags'].each do |flag_key, flag_value|
    unless flag_value == ''
        config = config + "-#{flag_key}=#{flag_value} "
    end
end
%>
exec <%= node['prometheus']['dir'] %>/prometheus <%= config %>
